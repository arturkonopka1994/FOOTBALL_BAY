<div class="banner_home background-shadow">
  <div class="home-video-wrapper">
    <!-- <video loop muted autoplay preload="auto">
      <source src="video.mp4"  type="video/mp4">
    </video> -->
    <div class="white-text" style="position: absolute; top: 16px; right:32px;">
      <p>
        <a href="#instruction" class="btn button-secondary-home button-match btn-lg list-inline", style="width: 400px;">Find out about us</a>
      </p>
    </div>
    <h1 class="home">Welcome to the <span style="color: #2BFF8B">Football Bay</span>!</h1>
    <%= video_tag "video.mp4", loop: :loop, muted: :muted, autoplay: :autoplay, width: '100%' , class: "home_video" %>
    <h1 class="home">Welcome to the Football Bay!</h1>
  </div>
  <div class="container">
    <div class="form-search">
      <ul class="list-inline d-flex justify-content-center white-text"> 
        <%= form_tag matches_path, method: :get do %>
          <li class="list-inline-item"><%= text_field_tag :by_address, params[:by_address], class: "form-control", placeholder: "Postcode or City", style: "width: 400px; height: 46px; border-radius: 8px; text-align: center; justify-content: center; margin-bottom: 0px;"%></li>    
          <li class="list-inline-item">
            <%= submit_tag "Search For The Game", class: "btn button button-match btn-lg list-inline", style: "width: 400px; margin-bottom: 0px; margin-left: 150px" %>
          </li>
        <% end %>
      </ul>
      <ul class="list-inline d-flex justify-content-around white-text">
        <li class="list-inline-item">
          <%= radio_button_tag :radius, 1, params[:radius] %>
          <p>1km</p>
        </li>
        <li class="list-inline-item">
          <%= radio_button_tag :radius, 3, params[:radius] %>
          <p>3km</p>
        </li>
        <li class="list-inline-item">
          <%= radio_button_tag :radius,5, params[:radius] %>
          <p>5km</p>
        </li>
        <li class="list-inline-item">
          <%= radio_button_tag :radius,10, params[:radius] %>
          <p>10km</p>
        </li>
      </ul>
    </div>
  </div>
</div>
